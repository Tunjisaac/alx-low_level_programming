#include "main.h"
/**
 * times_table -  it prints the 9 times table, starting with 0
 * rowf = row, colf = column, d = numbers of current result
 * Return: times table
 * add extra space past single digit
 */
void times_table(void)
{
	int rowf, colf, d;

	for (rowf = 0; rowf <= 9; rowf++)
	{
		_putchar('0');
		_putchar(',');
		_putchar(' ');
		for (colf = 1; colf <= 9; colf++)
		{
			d = (rowf * colf);
			if ((d / 10) > 0)
			{
				_putchar((d / 10) + '0');
			}
			else
			{
				_putchar(' ');
			}
			_putchar((d % 10) + '0');

			if (colf < 9)
			{
				_putchar(',');
				_putchar(' ');
			}
		}
		_putchar('\n');
	}

}
